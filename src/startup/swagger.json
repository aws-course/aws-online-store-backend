{
  "swaggerDefinition": {
    "info": {
      "description": "AWS course online store backend",
      "title": "Swagger",
      "version": "1.0.0"
    },
    "host": "localhost:8080",
    "basePath": "/api/v1",
    "produces": ["application/json"],
    "schemes": ["http"],
    "securityDefinitions": {
      "JWT": {
        "type": "apiKey",
        "in": "header",
        "name": "Authorization",
        "description": ""
      }
    }
  },
  "basedir": "./",
  "files": ["./src/**/*.js"]
}
